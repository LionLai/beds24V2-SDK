{
  "name": "beds24-api-v2-sdk",
  "version": "0.1.0",
  "description": "TypeScript client for the Beds24 API V2, ready for Nuxt and Next apps.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "openapi-typescript apiV2.yaml --output src/generated/beds24.ts",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "beds24",
    "sdk",
    "typescript",
    "nuxt",
    "nextjs",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openapi-fetch": "^0.9.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "openapi-typescript": "^7.5.3",
    "tsup": "^8.0.1",
    "typescript": "^5.5.3"
  }
}
